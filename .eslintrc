{
    "parser": "babel-eslint",
    "extends": ["airbnb"],
    "env": {
        "browser": true
    },
    "rules":{
        "no-console": 0,
        "semi": [2, "always"],
        "comma-dangle": [2, "never"],
        "import/no-extraneous-dependencies": 0,
        "import/extensions": 0,
        "import/first": 0,
        "no-use-before-define": 0,
        "consistent-return": 0,
        "no-plusplus": 0,
        "no-underscore-dangle": 0,
        "no-mixed-operators": 0,
        "no-param-reassign": [
            "error",
            { "props": false }
        ],
        "no-cond-assign": 0,
        "jsx-a11y/anchor-is-valid": 0
    }
}
